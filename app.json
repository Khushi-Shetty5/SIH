{
  "expo": {
    "name": "try",
    "slug": "try",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",

    
    "newArchEnabled": false,

    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "plugins": [
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "android": {
            "gradlePluginVersion": "8.0.2",
            "kotlinVersion": "1.9.10"
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],

    "cli": {
      
      "appVersionSource": "remote"
    },

    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera for video calls.",
        "NSMicrophoneUsageDescription": "This app uses the microphone for audio in video calls."
      },
      "bundleIdentifier": "com.anonymous.xtry"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "package": "com.anonymous.xtry"
    },

    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
