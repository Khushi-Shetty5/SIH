{
  "expo": {
    "name": "healthcare-dashboard",
    "slug": "healthcare-dashboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-asset",
      "expo-font",
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "android": {
            "gradlePluginVersion": "8.0.2",
            "kotlinVersion": "1.9.22"
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "cli": {
      "appVersionSource": "remote"
    },
    "ios": {
      "supportsTablet": true,
      "jsEngine": "hermes",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera access is needed for video consultations. This app uses the camera for video calls.",
        "NSMicrophoneUsageDescription": "This app uses the microphone for audio in video calls."
      },
      "bundleIdentifier": "com.anonymous.xtry"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "jsEngine": "hermes",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "package": "com.anonymous.xtry"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "60e8af83-77f7-44be-aefb-6a9f673afb56"
      }
    }
  }
}
